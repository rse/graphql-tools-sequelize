{
    "esnext":        true,
    "asi":           true,
    "maxerr":        200,
    "bitwise":       true,
    "camelcase":     false,
    "curly":         false,
    "eqeqeq":        true,
    "forin":         false,
    "immed":         true,
    "latedef":       true,
    "newcap":        false,
    "noarg":         false,
    "noempty":       false,
    "nonew":         true,
    "plusplus":      false,
    "quotmark":      "double",
    "regexp":        false,
    "undef":         true,
    "unused":        true,
    "strict":        false,
    "trailing":      true,
    "maxparams":     9,
    "maxdepth":      8,
    "maxstatements": 150,
    "maxlen":        200,
    "loopfunc":      true,
    "-W014":         false,
    "globals": {
        "Buffer":       false,
        "Promise":      false,
        "process":      false,
        "setTimeout":   false,
        "setInterval":  false,
        "clearTimeout": false,
        "global":       false,
        "__dirname":    false,
        "console":      false,
        "require":      false,
        "module":       false,
        "describe":     false,
        "it":           false,
        "expect":       false
    }
}
